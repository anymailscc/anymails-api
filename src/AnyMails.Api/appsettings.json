{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "Properties": {
      "Application": "AnyMails.Api"
    }
  },
  "Seq": {
    "HostName": "http://localhost:5081",
    "ApiKey": "xxx"
  },
  "Redis": {
    "Host": "localhost",
    "Port": "6379"
  }
}
